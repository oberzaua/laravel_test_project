  "version": "0.2.0",
  "configurations": [
    {
      "name": "XDebug Remote php",
      "type": "php",
      "request": "launch",
      "port": 9003,
      "log": true,
      //"hostname": "192.168.63.129",
      "pathMappings": {
        "/var/www/html": "${workspaceFolder}/"
      },
      "ignore": ["**/vendor/**/*.php"]
    },
    {
      "name": "XDebug Remote laravel",
      "type": "php",
      "request": "launch",
      "port": 9003,
      "log": true,
      //"hostname": "192.168.63.129",
      "pathMappings": {
        "/var/www/htmllaravel": "${workspaceFolder}/"
      },
      "ignore": ["**/vendor/**/*.php"]
    }
  ]
}
